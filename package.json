{
  "name": "rotativo-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -c local -o --port 5010",
    "build": "ng build",
    "test": "ng test --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "browser": "ng test --browsers=Chrome"
  },
  "browser": {
    "zlib": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "ng lint && ng test --code-coverage",
      "pre-push": "ng build -c qa",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.1",
    "@angular/common": "^17.0.1",
    "@angular/compiler": "^17.0.1",
